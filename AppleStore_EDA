            "source": [
                "SELECT TOP 10\n",
                "    prime_genre,\n",
                "    AVG(user_rating) AS AvgRating\n",
                "FROM \n",
                "    Projects.dbo.AppleStore\n",
                "GROUP BY\n",
                "    prime_genre\n",
                "ORDER BY \n",
                "    AvgRating"
            ],
            "metadata": {
                "language": "sql",
                "azdata_cell_guid": "3687be1f-a5f0-4be2-9d66-2fbbf0941e2e",
                "tags": []
